   <tr t:type="loop" t:source="cpsdata" t:value="campaign" class="gray">
      <td><a onclick="#" href="#">${campaign.name}</a></td>
      <td>${campaign.cookieMaxage}</td>
      <td><a href=""></a></td>
      <td><a href="#">${campaign.site.url}</a></td>
      <td><a href="#"></a></td>
      <td>${campaign.region}</td>
      <td>${campaign.actived}</td>
      <td></td>
      <td>${campaign.startDate}</td>
      <td>${campaign.endDate}</td>

      <td><select name="jumpMenu" id="jumpMenu" onchange="MM_jumpMenu('parent',this,0)">
        <option>请选择操作</option>
        <option>查看佣金</option>
        </select></td>
    </tr>
</table>    
