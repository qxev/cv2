<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">	<div style="height:120px;min-width:160px;display:table-cell;vertical-align:middle;">		<t:if test="prop:banner.bannerType">			<span t:type="t5components/Equals" left="prop:banner.bannerType.toString()" right="literal:0">				<img src="${banner.content}" width="${bannerWidth}" height="${bannerHeight}"/>	   		</span>	   		<span t:type="t5components/Equals" left="prop:banner.bannerType.toString()" right="literal:1">				<iframe src="/public/uploads/c${banner.campaign.id}/${banner.campaign.id}_${banner.id}.html" style="border: #ddd 1px solid; width: ${bannerWidth}px; height: ${bannerHeight}px;">${banner.content}</iframe>	   		</span>	   		<span t:type="t5components/Equals" left="prop:banner.bannerType.toString()" right="literal:2">	   			<script language="javascript" src="/assets/javascripts/swfobject.js"></script>	   			<div id="show_banner_${banner.id}"></div>				<script type="text/javascript">					var so = new SWFObject("${banner.content}", "myflash_${banner.id}", "${bannerWidth}", "${bannerHeight}", "7");					so.write("show_banner_${banner.id}");				</script>	   		</span>	   		<span t:type="t5components/Equals" left="prop:banner.bannerType.toString()" right="literal:3">				<iframe src="/public/uploads/c${banner.campaign.id}/${banner.campaign.id}_${banner.id}.html" style="border: #ddd 1px solid; width: ${bannerWidth}px; height: ${bannerHeight}px;">${banner.content}</iframe>	   		</span>	   		<span t:type="t5components/Equals" left="prop:banner.bannerType.toString()" right="literal:4">				<img src="${banner.content}" width="${bannerWidth}" height="${bannerHeight}"/>	   		</span>	   	</t:if>   	</div></html>